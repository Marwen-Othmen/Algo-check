Algorithm Check Point
var 
n: INTEGER := 0 (nbr mots)
J: INTEGER :=0 (nbr Alphabet )
I: INTEGER :=0(nbr vowels)
V: INTEGER :=0
C: CHAR

Begin 
Read (c); 
WHILE (c<> ".") DO
    if (c=" ") then 
    n=n+1;
    end if 
    if (c<> " ") then
    j=j+1; 
    end if 
    if (c=[a,A,e,E,i,I,o,O,u,U,y,Y]);
    v=v+1;
    end if 
    write ( n+1);
    write ( j );
    write ( V);
END_WHILE